# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# 📊 Google Analytics 配置
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Google Analytics Measurement ID（生产环境）
# 如何获取：
# 1. 登录 https://analytics.google.com
# 2. Admin → Data Streams → Web → 获取 Measurement ID
# 3. 格式：G-XXXXXXXXXX
#
# 用途：
# - 收集用户行为数据（页面浏览、路由跳转）
# - 为 Guess.js 提供训练数据
#
# 不设置：构建时会显示警告，不影响应用运行
# 设置后：自动在生产环境启用 GA 追踪
# GA_MEASUREMENT_ID=G-XXXXXXXXXX

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# 🔮 Guess.js 配置
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# Google Analytics View ID（可选，用于 Guess.js）
# 如何获取：
# 1. 登录 Google Analytics
# 2. Admin → View Settings → View ID
# 3. 复制类似 "123456789" 的数字
#
# 注意：这是用于 Guess.js 的 View ID，与上面的 Measurement ID 不同
#
# 不设置此变量：自动降级到静态配置 + Quicklink
# 设置后：Guess.js 自动从 GA 获取数据训练 ML 模型
# GUESS_GA_VIEW_ID=123456789

# 自定义数据源 API（可选，优先级低于 GA）
# 如果你有自己的用户行为分析后端，可以配置这个 URL
# API 应返回 Guess.js 兼容的数据格式
# GUESS_REPORT_URL=https://api.yoursite.com/analytics/routes

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# 🐛 调试配置
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━

# 启用 Guess.js 调试模式
# 开启后会在构建时和运行时输出详细日志
# DEBUG=true
